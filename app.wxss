/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{background:#f9f9f9;font-size:24rpx;padding-bottom: 20rpx}
.checkbox{color: #999;}
.checkbox navigator {display: inline-block;text-decoration: underline;color: #479ef8;margin-top:20rpx}
.fwtk .wx-checkbox-input {
      margin-right: 15rpx;
      width: 25rpx;
      height: 25rpx;
      background: #f5f5f5  !important;
      border-radius: 4rpx;
      box-sizing: border-box;
      border-color: #ccc !important;
      position: relative;
      top: -4rpx;
}
.fwtk .wx-checkbox-input::before {
      width: 25rpx;
      height: 25rpx;
      background: #f5f5f5;
      border-color: #ccc !important;
      border-radius: 4rpx;
      box-sizing: border-box;
}
.fwtk .wx-checkbox-input.wx-checkbox-input-checked {
      background: #f5f5f5;
      border-color:#ccc !important;
      box-sizing: border-box;
}
.fwtk .wx-checkbox-input.wx-checkbox-input-checked::before {
      font-size: 25rpx;
      color: #333;
}