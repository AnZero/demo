.own-top{height: 150rpx;width: 100%;background: #4e95f1;position: relative;}
.own-top>image{width: 90rpx;height: 90rpx;margin: 7rpx 0 0 30rpx;float: left;border: 4rpx solid #fff;border-radius: 100%;background: #fff;}
.own-top>text{color: #fff;font-weight: bold;font-size: 16px;float: left;line-height: 100rpx;margin-left: 30rpx;}

.own-cen{width: 698rpx;height: 200rpx;background: #fff;border-radius: 20rpx;margin: 0 auto;margin-top: -30rpx;overflow: hidden;position: relative;}
.own-cen navigator image{width: 13rpx;height: 23rpx;float: right;margin-right: 30rpx;margin-top: 38rpx;}
.own-cen navigator{display: block;height: 100rpx;line-height: 100rpx;}
.own-cen navigator view{float: left;height: 100rpx;line-height: 100rpx;margin-left: 30rpx;font-size: 13px;position: relative;}
.own-cen .act view::after{content: '';position: absolute;width: 16rpx;height: 16rpx;border-radius: 100%;background: #E83015;right: -510rpx;top: 41rpx;}

.login{height: 60rpx;width: 150rpx;border-radius: 60rpx;border: 2rpx solid #4e95f1;color: #4e95f1;text-align: center;line-height: 60rpx;font-size: 14px;margin-top: 50rpx;}
.own-cen>view{text-align: center;color: #999;margin-top: 30rpx;}

.set{position: absolute;width: 40rpx;height: 40rpx;right: 40rpx;top: 34rpx;z-index: 2}
.set>image{width: 40rpx;height: 40rpx;}